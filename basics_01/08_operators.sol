


contract MyContract {
    function add(uint a, uint b) external pure returns (uint) {
        return a + b;
    }

    function subtract(uint a, uint b) external pure returns (uint) {
        return a - b;
    }

    function multiply(uint a, uint b) external pure returns (uint) {
        return a * b;
    }

    function divide(uint a, uint b) external pure returns (uint) {
        return a / b;
    }

    function modulus(uint a, uint b) external pure returns (uint) {
        return a % b;
    }

    function exponentiation(uint a, uint b) external pure returns (uint) {
        return a ** b;
    }

    function square(uint a) external pure returns (uint) {
        return a * a;
    }

    function cube(uint a) external pure returns (uint) {
        return a * a * a;
    }    


